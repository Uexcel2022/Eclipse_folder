package GUI;

import javax.swing.JFrame;
import javax.swing.JDialog;
import javax.swing.JPanel;

@SuppressWarnings("serial")
public class FrameDemo extends JFrame {
	JFrame obj;
	public FrameDemo () {
		obj = new JFrame();
		obj.setTitle("JFrame Demo");
		obj.setVisible(true);
		obj.setSize(300, 300);
	}
	
	public static void main(String[] args) {
		JPanelDemo jp = new JPanelDemo();
		DialogDemo dl = new DialogDemo();
		FrameDemo fd = new  FrameDemo();
		 
		
	}

}

@SuppressWarnings("serial")
class DialogDemo extends JDialog{
	JDialog dl;
	public DialogDemo(){
		dl = new JDialog();
		dl.setTitle("JDialog");
		dl.setVisible(true);
		dl.setSize(300, 150);
	
	}
	
}

@SuppressWarnings("serial")
class JPanelDemo extends JFrame{
	JPanel pn;
	public JPanelDemo() {
		pn = new JPanel();
		setTitle("JPanelDemo");
		setVisible(true);
		setSize(300, 200);
		add(pn);
	}
}





